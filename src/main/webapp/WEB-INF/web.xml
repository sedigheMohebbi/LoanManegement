<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <servlet>
        <servlet-name>LegalCustomerServlet</servlet-name>
        <servlet-class>presentation.LegalCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoanServlet</servlet-name>
        <servlet-class>presentation.LoanServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoanTypeServlet</servlet-name>
        <servlet-class>presentation.LoanTypeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RealCustomerServlet</servlet-name>
        <servlet-class>presentation.RealCustomerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LegalCustomerServlet</servlet-name>
        <url-pattern>/LegalCustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoanServlet</servlet-name>
        <url-pattern>/LoanServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RealCustomerServlet</servlet-name>
        <url-pattern>/RealCustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoanTypeServlet</servlet-name>
        <url-pattern>/LoanTypeServlet</url-pattern>
    </servlet-mapping>
</web-app>
